load("@io_bazel_rules_go//go:def.bzl", "go_library")
load("@io_bazel_rules_go//proto:def.bzl", "go_proto_library")

proto_library(
    name = "token_proto",
    srcs = ["token.proto"],
    visibility = ["//visibility:public"],
)

go_proto_library(
    name = "token_go_proto",
    importpath = "github.com/bazelbuild/tools_jvm_autodeps/jadep/thirdparty/golang/parsers/public/token",
    proto = ":token_proto",
    visibility = ["//visibility:public"],
)

go_library(
    name = "go_default_library",
    embed = [":token_go_proto"],
    importpath = "github.com/bazelbuild/tools_jvm_autodeps/jadep/thirdparty/golang/parsers/public/token",
    visibility = ["//visibility:public"],
)
